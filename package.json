{
  "name": "secretary",
  "description": "Wechat secretary",
  "version": "0.0.0",
  "private": true,
  "main": "dist/background.js",
  "build": {
    "appId": "nnecec.secretary",
    "productName": "secretary",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "dev:electron": "cross-env NODE_ENV=development electron -r @babel/register ./src/background.js",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "pree2e": "webpack --config=build/webpack.config.prod.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "release": "cross-env NODE_ENV=production webpack --config=build/webpack.config.prod.js && electron-builder",
    "start": "node build/start.js",
    "test": "npm run unit && npm run e2e",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register"
  },
  "dependencies": {
    "@rematch/core": "^1.1.0",
    "@rematch/immer": "^1.1.0",
    "@rematch/loading": "^0.5.1",
    "compressorjs": "^1.0.5",
    "connected-react-router": "^6.3.2",
    "evergreen-ui": "^4.15.0",
    "file-saver": "^2.0.1",
    "fs-jetpack": "^2.2.2",
    "immer": "^2.1.5",
    "jszip": "^3.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-mocha": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.1.1",
    "node-loader": "^0.6.0",
    "react-hot-loader": "^4.8.3",
    "source-map-support": "^0.5.11",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
